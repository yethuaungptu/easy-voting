        <!-- campaign list start  -->
        <div class="page-body">
          <div class="container-xl">
            <div class="row row-cards">
              <div class="col-sm-6 col-lg-4">
                <div class="card card-sm">
                  <a href="#" class="d-block"><img src="/images/profile.jpg" class="card-img-top"></a>
                    <div class="card-body">
                      <div class="row">
                          <div class="col-6">
                            <div class="campaign-name">Pawe≈Ç Kuna</div>
                          </div>
                          <div class="col-6">
                            <div class="row">
                              <div class="col-6">
                              <input type="button" value="Detail" class="btn btn-primary">
                              </div>
                              <div class="col-6">
                              <input type="button" value="Delete" class="btn btn-danger">
                              </div>
                            </div>
                          </div>
                      </div>
                    </div>
                </div>
              </div>
            </div>
          </div>
        </div>
